    "N = 5\n",
    "S = 4\n",
    "str_digits = \"\".join([f\"{d}\" for d in range(N+1)])\n",
    "config_arr = []\n",
    "for perm in product(str_digits, repeat=S):\n",
    "  int_prod = list(map(lambda d: int(d), perm))\n",
    "  if sum(int_prod) == N:\n",
    "    config_arr.append(int_prod)\n",
